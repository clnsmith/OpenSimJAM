<?xml version="1.0" encoding="UTF-8" ?>
<OpenSimDocument Version="40000">
    <JointMechanicsTool>
        <!--Path to .osim file for analysis.-->
        <model_file>../../../models/lenhart2015/lenhart2015.osim</model_file>
        <!--Motion file (.mot) or storage file (.sto) containing the time history of the generalized coordinates for the model.-->
        <coordinates_file>./results/passive_flexion_values.sto</coordinates_file>
        <!--Folder where the results files are output.-->
        <results_directory>./results/contact</results_directory>
        <!--Prefix to each results file name.-->
        <results_file_basename>passive_flexion</results_file_basename>
        <!--Time to start analysis. Set to -1 to use initial frame in coordinates_file-->
        <start_time>-1</start_time>
        <!--Time to stop analysis. Set to -1 to use last frame in coordinates_file-->
        <stop_time>-1</stop_time>
        <!--Time step size to report results, set to -1 to use steps directly from coordinates_file-->
        <resample_step_size>-1</resample_step_size>
        <!--Resample to 101 equally spaced time steps. Note: If true, this overrides resample_step_size.-->
        <normalize_to_cycle>false</normalize_to_cycle>
        <!--Apply a 5 order smoothing spline to input coordinates, set to -1 to use raw data.NOTE: lowpass filter is NOT applied if smoothing_spline_frequency > -1-->
        <smoothing_spline_frequency>-1</smoothing_spline_frequency>
        <!--Order of the smoothing spline.-->
        <smoothing_spline_order>5</smoothing_spline_order>
        <!--Apply IIR lowpass butterworth filter the input coordinates, set to -1 to use raw data-->
        <lowpass_filter_frequency>-1</lowpass_filter_frequency>
        <!--Print a .sto file with the processed kinematics used for posing the analysis.-->
        <print_processed_kinematcs>false</print_processed_kinematcs>
        <!--Names of WISCO_ElasticFoundation contacts to be recorded.-->
        <contact_names>all</contact_names>
        <!--Include face pressure values in .h5 and .vtp files-->
        <output_pressure>true</output_pressure>
        <!--Include face proximity values in .h5 and .vtp files-->
        <output_proximity>true</output_proximity>
        <!--Format of output data 'face', 'vertex' or 'both'.-->
        <output_data_mesh_format>face</output_data_mesh_format>
        <!--Write .vtp files visualization.-->
        <write_vtp_files>true</write_vtp_files>
        <!--Origin for mesh motion in .vtp files.Options: 'ground' or 'body_name' for dynamic files, 'static' for the contact meshes in local reference frame.-->
        <vtp_origin>ground</vtp_origin>
        <!--Reference frame for mesh motion in .vtp files.Options: 'ground' or 'body_name' for dynamic files, 'static' for the contact meshes in local reference frame.-->
        <vtp_frame>ground</vtp_frame>
        <!--Generate .vtk files for attached_geometry meshes for visualization.Options: 'none','all', or a list of frame names.-->
        <vtp_attached_geometries>none</vtp_attached_geometries>
        <!--Output ligament lines of action, strain, and force data to .vtp files-->
        <vtp_ligaments>true</vtp_ligaments>
        <!--Output muscle lines of action, strain, and force data to .vtp files-->
        <vtp_muscles>true</vtp_muscles>
        <!--Add cartilage thickness and material property maps to vtp files.Options: 'none','thickness','elastic modulus','poisson ratio' or 'all'-->
        <vtp_contact_properties>none</vtp_contact_properties>
        <!--Write detailed contact data to .h5 file-->
        <h5_raw_contact_data>true</h5_raw_contact_data>
        <!--Write summary statistics of contact to .h5 file.-->
        <h5_summary_contact_data>true</h5_summary_contact_data>
        <!--Write medial-lateral regional summary statistics to .h5 file-->
        <h5_medial_lateral_summary>true</h5_medial_lateral_summary>
        <!--Write binary .h5 file-->
        <write_h5_file>true</write_h5_file>
        <!--Write states data to .h5 file-->
        <h5_states_data>true</h5_states_data>
        <!--Write kinematics data to .h5 file-->
        <h5_kinematics_data>true</h5_kinematics_data>
        <!--Write muscle analysis data to .h5 file-->
        <h5_muscle_data>true</h5_muscle_data>
        <!--Write Blankevoort1991_Ligament data to h5 file-->
        <h5_ligament_data>true</h5_ligament_data>
        <!--Write transformation matrix -->
        <write_blender_csv>false</write_blender_csv>
    </JointMechanicsTool>
</OpenSimDocument>
